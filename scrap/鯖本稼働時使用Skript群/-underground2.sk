command /ts:
	usage: /error
	permission: sk.itemget
	trigger:
		set {_} to 1
		loop 10 times:
			hougu(player,player,"none",{_},3)
			give {ug-item-t.%player%} to player
			add 1 to {_}
on rightclick:
	if lore of player's tool contain "宝具":
		hougu(player,player,"%name of player's tool%",0,1)
		
command /underground-item [<player=%player%>] [<text>] [<text>] [<number = 1>] [<number = 1>]:
	permission:console.console
	trigger:
		if arg 2 = "shop":
			open chest with 1 row named "&cべに&f%{ug.aka.%arg 1%}%:&bあお&f%{ug.ao.%arg 1%}%:&aみどり&f%{ug.midori.%arg 1%}%" to arg 1
			wait a tick
			set {_slot} to 0
			loop 3 times:
				set {_num} to random integer between 1 and 9
				set {_num2} to random integer between -50 and 50
				items(arg 1 ,arg 1, {_num}, 1, 0)
				set {_sell} to items(arg 1 ,arg 1, {_num}, 2, 0)
				add {_num2} to {_sell}
				format slot {_slot} of arg 1 with {ug-item-t.%arg 1%} named "&f%{ug-item-d.%arg 1%}%" with lore "&cべにタマ&f%{_sell}%&a必要" to close then run "underground-item %arg 1% buy red %{_num}% %{_sell}%"
				add 1 to {_slot}
			loop 3 times:
				set {_num} to random integer between 10 and 19
				set {_num2} to random integer between -50 and 80
				items(arg 1 ,arg 1, {_num}, 1, 0)
				set {_sell} to items(arg 1 ,arg 1, {_num}, 2, 0)
				add {_num2} to {_sell}
				format slot {_slot} of arg 1 with {ug-item-t.%arg 1%} named "&f%{ug-item-d.%arg 1%}%" with lore "&bあおタマ&f%{_sell}%&a必要" to close then run "underground-item %arg 1% buy blue %{_num}% %{_sell}%"
				add 1 to {_slot}
			loop 3 times:
				set {_num} to random integer between 1 and 10
				set {_num2} to random integer between -100 and 200
				set {_sell} to hougu(arg 1 ,arg 1, "none", {_num}, 3)
				add {_num2} to {_sell}
				format slot {_slot} of arg 1 with {ug-item-t.%arg 1%} named "&f%{ug-item-d.%arg 1%}%" with lore "&aみどりタマ&f%{_sell}%&a必要" to close then run "underground-item %arg 1% buy green %{_num}% %{_sell}%"
				add 1 to {_slot}
		else if arg 2 = "buy":
			if arg 3 = "red":
				if {ug.aka.%arg 1%} is more than arg 5:
					add -1 * arg 5 to {ug.aka.%arg 1%}
					items(arg 1 ,arg 1, arg 4, 1, 1)
					message "{@tag}アイテムを購入した" to arg 1
				else:
					message "{@tag}コストが足りないようだ" to arg 1
			else if arg 3 = "blue":
				if {ug.ao.%arg 1%} is more than arg 5:
					add -1 * arg 5 to {ug.ao.%arg 1%}
					items(arg 1 ,arg 1, arg 4, 1, 1)
