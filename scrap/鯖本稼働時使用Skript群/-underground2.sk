function hougu(p1:player ,p2:player ,name:text ,n:number, use:number) :: number:
	if {_n} = 0:
		if {_name} is "&3越後屋のお菓子":
			set {_n} to 1
		else if {_name} is "&e星の雫":
			set {_n} to 2
		else if {_name} is "&4ボンバーナックル":
			set {_n} to 3
		else if {_name} is "&a四季の思い出":
			set {_n} to 4
		else if {_name} is "&4真実の鏡":
			set {_n} to 5
		else if {_name} is "&6ツタンカーム":
			set {_n} to 6
		else if {_name} is "&4覇王のマント":
			set {_n} to 7
		else if {_name} is "&a月の浮舟":
			set {_n} to 8
		else if {_name} is "&b漣の笛":
			set {_n} to 9
		else if {_name} is "&a魔法の絵筆":
			set {_n} to 10
			
	if {_n} = 1:
		set {ug-item-t.%{_p1}%} to pumpkin pie of fortune 1 named "&3越後屋のお菓子" with lore "&b[防御宝具]||&6宝具による攻撃を１回防ぎます"
		set {ug-item-d.%{_p1}%} to "&3越後屋のお菓子"
		if {_use} = 1:
			message "{@tag}所持しているだけで効果があるアイテムです" to {_p1}
		else if {_use} = 3:
			return 420
		
	else if {_n} = 2:
		set {ug-item-t.%{_p1}%} to ghast tear of fortune 1 named "&e星の雫" with lore "&b[特殊宝具]||&6HPと満腹度を回復します"
		set {ug-item-d.%{_p1}%} to "&e星の雫"
		if {_use} = 1:
			remove 1 of {_p1}'s tool from {_p1}
			message "{@tag}体力と満腹度を回復しました" to {_p1}
			set {_p1}'s health to 10
			set {_p1}'s food level to 10
		else if {_use} = 3:
			return 300
			
	else if {_n} = 3:
		set {ug-item-t.%{_p1}%} to flint of fortune 1 named "&4ボンバーナックル" with lore "&b[攻撃宝具]||&6ランダムなプレイヤーから||&6鉄を5奪います"
		set {ug-item-d.%{_p1}%} to "&4ボンバーナックル"
		if {_use} = 1:
			loop all players:
				if {ug-in.%loop-player%} is set:
					if loop-player is not {_p1}:
						set {_num.%loop-player%} to random integer between 1 and 100
			set {_max} to 0
			loop all players:
				if {_num.%loop-player%} is set:
					if {_max} is less than {_num.%loop-player%}:
						set {_max} to {_num.%loop-player%}
						set {_maxp} to loop-player
			remove 1 of {_p1}'s tool from {_p1}
			if {_maxp} is not set:
				message "{@tag}対象にできるプレイヤーがいなかった" to {_p1}
				stop
			else:
				hougu({_maxp}, {_p1}, "none", {_n}, 2)
		else if {_use} = 2:
			loop items in {_p1}'s inventory:
				if lore of loop-item contain "防御":
					remove 1 of loop-item from {_p1}
					message "{@tag}宝具により攻撃を防いだ" to {_p1}
					message "{@tag}宝具によって防がれたようだ" to {_p2}
					if name of loop-item contain "真実":
						message "{@tag}真実の鏡の効果を発動" to {_p1}
						hougu({_p2},{_p1},"none",3,2)
						hougu({_p2},{_p1},"none",3,2)
					stop
			message "{@tag}宝具による攻撃で鉄を奪われた" to {_p1}
			message "{@tag}鉄を５つ奪った" to {_p2}
			remove 5 of iron bar from {_p1}
			give 5 iron bar to {_p2}
		else if {_use} = 3:
			return 600
	else if {_n} = 4:
		set {ug-item-t.%{_p1}%} to nether star of fortune 1 named "&a四季の思い出" with lore "&b[防御宝具]||&6宝具による攻撃を１回防ぎます"
