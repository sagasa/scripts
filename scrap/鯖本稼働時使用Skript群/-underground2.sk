on rightclick with name tag:
	set {_ts::*} to name of player's tool parsed as "%text% 耐久:%number%"
	if {_ts::*} is set:
		set {_num} to {_ts::2} - 1
		set name of player's tool to "%{_ts::1}% 耐久:%{_num}%"
		execute console command "underground %player% radar"
		if {_num} is less than 1:
			message "{@tag}レーダーが壊れてしまった"
			remove 1 of player's tool from the player
			
#鉱石クリック
on leftclick with name tag:
	cancel event
	set {_loc} to location of event-block
	add -3 to y-coordinate of {_loc}
	if block at {_loc} is gold block:
		message "{@tag}かべがすこしもりあがっている"
		set block at {_loc} to air
		wait 1 seconds
		execute console command "underground createore"
		execute console command "underground %player% go"
	else:
		message "{@tag}鉱石は見つからなかった"
	set {_ts::*} to name of player's tool parsed as "%text% 耐久:%number%"
	if {_ts::*} is set:
		set {_num} to {_ts::2} - 1
		set name of player's tool to "%{_ts::1}% 耐久:%{_num}%"
		if {_num} is less than 1:
			message "{@tag}レーダーが壊れてしまった"
			remove 1 of player's tool from the player
