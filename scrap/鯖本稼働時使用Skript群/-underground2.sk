		if {_use} = 1:
			message "{@tag}所持しているだけで効果があるアイテムです" to {_p1}
		else if {_use} = 3:
			return 600
	else if {_n} = 6:
		set {ug-item-t.%{_p1}%} to raw pufferfish of fortune 1 named "&6ツタンカーム" with lore "&b[宝具]"
		set {ug-item-d.%{_p1}%} to "&6ツタンカーム"
		if {_use} = 1:
			message "{@tag}効果の持たない宝具です" to {_p1}
		else if {_use} = 3:
			return 200
	else if {_n} = 7:
		set {ug-item-t.%{_p1}%} to leather of fortune 1 named "&4覇王のマント" with lore "&b[宝具]"
		set {ug-item-d.%{_p1}%} to "&4覇王のマント"
