command /summit <number> <number> <number>:
	permission:console.console
	trigger:
		loop all players:
			if loop-player is not op:
				execute console command "/tp %arg 1%  %arg 2% %arg 3%"

command /editgm <number>:
	permission:console.console
	trigger:
		loop all players:
			execute console command "/gamemode %loop-player% %arg 1%"
