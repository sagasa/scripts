				execute console command "/scoreboard players reset &b■■&a■■■■■■&8%arg-1%."
				execute console command "/scoreboard players reset &b■■■&a■■■■■&8%arg-1%"
				execute console command "/scoreboard players reset &b■■■&a■■■■■&8%arg-1%."
